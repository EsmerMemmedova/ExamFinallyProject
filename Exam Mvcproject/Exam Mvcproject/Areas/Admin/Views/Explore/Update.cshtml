@using Core.Models;
@model Explore
<div class="container">
<form method="post" enctype="multipart/form-data">
 <div class="row">
      <div class="mb-3 col-md-4">
    <label asp-for="FullName" class="form-label">FullName</label>
    <input asp-for="FullName" class="form-control">
    <span asp-validation-for="FullName" style="color:red"></span>
 
  </div>
  <div class="mb-3 col-md-4">
                <label asp-for="Description" class="form-label">Position</label>
                <input asp-for="Description" class="form-control">
                <span asp-validation-for="Description" style="color:red"></span>
  </div>
   <div class="mb-3 col-md-4">
            <label asp-for="ImgFile" class="form-label">ImgFile</label>
            <input asp-for="ImgFile" class="form-control">
                <span asp-validation-for="ImgFile" style="color:red"></span>
   </div>
 </div>
 <div asp-validation-summary="ModelOnly" style="text-align-danger"></div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>